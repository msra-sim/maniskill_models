  <!-- ============================= -->
  <!-- Planning Group (arm only)     -->
  <!-- ============================= -->
  <group name="left_arm">
    <chain base_link="left_base_link" tip_link="arm_l_end_link" />
  </group>

  <group name="left_hand">
        <chain base_link="arm_l_end_link" tip_link="left_eef" />
    </group>

  <!-- Define the gripper as the end effector -->
  <end_effector name="left_gripper" parent_link="arm_l_end_link" group="left_hand" />
  
  <group name="right_arm">
    <chain base_link="right_base_link" tip_link="arm_r_end_link" />
  </group>

